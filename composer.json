{
    "name": "graham-campbell/testbench",
    "description": "TestBench Provides Some Testing Functionality For Laravel 4.1",
    "keywords": ["laravel", "framework", "test", "bench", "testbench", "TestBench", "Laravel TestBench", "Laravel-TestBench", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.1.*",
        "symfony/config": "2.4.*",
        "symfony/event-dispatcher": "2.4.*",
        "symfony/filesystem": "2.4.*",
        "symfony/stopwatch": "2.4.*",
        "symfony/yaml": "2.4.*",
        "guzzle/guzzle": "3.8.*",
        "orchestra/testbench": "2.1.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.0.*",
        "satooshi/php-coveralls": "0.7.*"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\TestBench\\": "src/",
            "GrahamCampbell\\Tests\\TestBench\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
