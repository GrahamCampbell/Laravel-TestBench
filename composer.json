{
    "name": "graham-campbell/testbench",
    "description": "TestBench Provides Some Testing Functionality For Laravel",
    "keywords": ["laravel", "framework", "testing", "testbench", "TestBench", "Laravel TestBench", "Laravel-TestBench", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "hello@gjcampbell.co.uk",
            "homepage": "https://github.com/GrahamCampbell"
        }
    ],
    "require": {
        "php": "^7.4.15 || ^8.0.2",
        "graham-campbell/testbench-core": "^4.0",
        "orchestra/testbench": "^6.25.1 || ^7.22.1 || ^8.0.4"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^4.0",
        "mockery/mockery": "^1.5.1",
        "phpunit/phpunit": "^9.5 || ^10.0"
    },
    "conflict": {
        "kylekatarnls/update-helper": "*"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\TestBench\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\TestBench\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
