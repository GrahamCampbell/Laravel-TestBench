{
    "name": "graham-campbell/testbench",
    "description": "TestBench Provides Some Testing Functionality For Laravel 4.2",
    "keywords": ["laravel", "framework", "test", "bench", "testbench", "TestBench", "Laravel TestBench", "Laravel-TestBench", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "~4.2.10",
        "orchestra/testbench": "2.2.*",
        "mockery/mockery": "~0.9.2",
        "phpunit/phpunit": "~4.3"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\TestBench\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\TestBench\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
