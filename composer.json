{
    "name": "graham-campbell/testbench",
    "description": "TestBench Provides Some Testing Functionality For Laravel 5",
    "keywords": ["laravel", "framework", "testing", "testbench", "TestBench", "Laravel TestBench", "Laravel-TestBench", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "graham-campbell/testbench-core": "^2.0",
        "orchestra/testbench": "3.1.*|3.2.*|3.3.*|3.4.*|3.5.*"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\TestBench\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\TestBench\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
